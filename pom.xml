<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>masecla</groupId>
	<artifactId>VillagerGUIApi</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>8</source>
					<target>8</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<repositories>
		<repository>
			<id>spigot-repo</id>
			<url>https://hub.spigotmc.org/nexus/content/repositories/snapshots/
			</url>
		</repository>
	</repositories>
	<dependencies>
		<dependency>
			<groupId>org.spigot1.12.2</groupId>
			<artifactId>craftbukkit1.12.2</artifactId>
			<version>1.12.2</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/spigot-1.12.2.jar
			</systemPath>
		</dependency>
		<dependency>
			<groupId>org.spigot1.9.4</groupId>
			<artifactId>craftbukkit1.9.4</artifactId>
			<version>1.9.4</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/spigot-1.9.4.jar
			</systemPath>
		</dependency>
		<dependency>
			<groupId>org.spigot1.9.2</groupId>
			<artifactId>craftbukkit1.9.2</artifactId>
			<version>1.9.2</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/spigot-1.9.2.jar
			</systemPath>
		</dependency>
		<dependency>
			<groupId>org.spigot1.8.8</groupId>
			<artifactId>craftbukkit1.8.8</artifactId>
			<version>1.8.8</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/spigot-1.8.8.jar
			</systemPath>
		</dependency>
		<dependency>
			<groupId>org.spigot1.8.3</groupId>
			<artifactId>craftbukkit1.8.3</artifactId>
			<version>1.8.3</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/spigot-1.8.3.jar
			</systemPath>
		</dependency>
		<dependency>
			<groupId>org.spigot1.8</groupId>
			<artifactId>craftbukkit1.8</artifactId>
			<version>1.8</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/spigot-1.8.jar
			</systemPath>
		</dependency>
		<dependency>
			<groupId>org.spigotmc</groupId>
			<artifactId>spigot-api</artifactId>
			<version>1.12.2-R0.1-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>
</project>